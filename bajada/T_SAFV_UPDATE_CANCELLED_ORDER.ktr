    <connection>V1</connection>
    <sql>select
	pn.meta_value ,
	p.ID,
	post_status
from
	${V1_TABLE_PREFIX}_posts p
left JOIN ${V1_TABLE_PREFIX}_postmeta pn on
	p.ID = pn.post_id
	and pn.meta_key = '_order_number'
where
	p.post_type = 'shop_order'
	and post_status in ('wc-cancelled',
	'wc-anulado',
	'trash');</sql>
